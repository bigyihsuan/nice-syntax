// fibonacci
function fibonacci(n as int) to int {
    match n {
        where n < 0 is -1,
        where 0 is 0,
        where 1 is 1,
        otherwise is (fibonacci(n-1) + fibonacci(n-2)),
    }
};

function main(argv as list of string) {
    // takes in 1 argument from stdin, the number n to fib
    print(fibonacci(str.toInt(argv at 1)));
}